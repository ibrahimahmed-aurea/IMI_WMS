<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Cdc.MetaManager.DataAccess.Domain.DeploymentGroup, Cdc.MetaManager.DataAccess" table="DeploymentGroup">
    <id name="Id" type="System.Guid">
      <column name="Id" not-null="true"/>
      <generator class="assigned"/>
    </id>
    <property name="Name" type="String">
      <column name="Name" length="100" not-null="true"/>
    </property>
    <many-to-one name="BackendApplication" class="Cdc.MetaManager.DataAccess.Domain.Application, Cdc.MetaManager.DataAccess" foreign-key="FK_DeploymentGroup_BE_Application">
      <column name="BackendApplicationId" not-null="true"/>
    </many-to-one>
    <many-to-one name="FrontendApplication" class="Cdc.MetaManager.DataAccess.Domain.Application, Cdc.MetaManager.DataAccess" foreign-key="FK_DeploymentGroup_FE_Application">
      <column name="FrontendApplicationId" not-null="true"/>
    </many-to-one>
    <property name="IsLocked" type="Boolean">
      <column name="IsLocked" not-null="false"/>
    </property>
    <property name="LockedBy" type="String">
      <column name="LockedBy" length="40" not-null="false"/>
    </property>
    <property name="LockedDate" type="System.DateTime">
      <column name="LockedDate" not-null="false"/>
    </property>
    <property name="State" type="Int32">
      <column name="State" not-null="true"/>
    </property>
  </class>
</hibernate-mapping>
