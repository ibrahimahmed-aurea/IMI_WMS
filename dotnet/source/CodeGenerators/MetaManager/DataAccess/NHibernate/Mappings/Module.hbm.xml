<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Cdc.MetaManager.DataAccess.Domain.Module, Cdc.MetaManager.DataAccess" table="Module">
    <id name="Id" type="System.Guid">
      <column name="Id" not-null="true"/>
      <generator class="assigned"/>
    </id>
    <property name="Name" type="String">
      <column name="Name" length="50" not-null="true"/>
    </property>
    <many-to-one name="Application" class="Cdc.MetaManager.DataAccess.Domain.Application, Cdc.MetaManager.DataAccess" foreign-key="FK_Module_Application">
      <column name="ApplicationId" not-null="true"/>
    </many-to-one>
    <bag name="Dialogs" inverse="true" lazy="true" cascade="none">
      <key column="ModuleId"/>
      <one-to-many class="Cdc.MetaManager.DataAccess.Domain.Dialog, Cdc.MetaManager.DataAccess"/>
    </bag>
    <bag name="Workflows" inverse="true" lazy="true" cascade="none">
      <key column="ModuleId"/>
      <one-to-many class="Cdc.MetaManager.DataAccess.Domain.Workflow, Cdc.MetaManager.DataAccess"/>
    </bag>
    <property name="IsLocked" type="Boolean">
      <column name="IsLocked" not-null="false"/>
    </property>
    <property name="LockedBy" type="String">
      <column name="LockedBy" length="40" not-null="false"/>
    </property>
    <property name="LockedDate" type="System.DateTime">
      <column name="LockedDate" not-null="false"/>
    </property>
    <property name="State" type="Int32">
      <column name="State" not-null="true"/>
    </property>
  </class>
</hibernate-mapping>
